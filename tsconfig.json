{
  "compilerOptions": {
    "strict": true,
    "module": "es2020",
    "target": "esnext",
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "lib": ["es2017", "dom"],
    "rootDir": "src",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "strictNullChecks": true,
    "noEmit": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "fixture", "src/resolver/fixture", "**/fixture/**"]
}
